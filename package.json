{
  "name": "narvalsyntax",
  "publisher": "wesuRage",
  "displayName": "Narval Syntax Highlight",
  "description": "Syntax Highlighting for the Narval programming language.",
  "repository": "https://github.com/wesuRage/narval-syntax-highlighting",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.91.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "nv",
        "aliases": [
          "narval",
          "nv"
        ],
        "extensions": [
          ".nv"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "nv",
        "scopeName": "source.nv",
        "path": "./syntaxes/narval.tmLanguage.json"
      }
    ]
  }
}
